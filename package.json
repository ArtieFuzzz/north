{
  "name": "north",
  "version": "1.0.0",
  "main": "dist/src/index",
  "author": "ArtieFuzzz#8298",
  "license": "MIT",
  "private": true,
  "description": "Discord bot using @sapphiredev/framework",
  "scripts": {
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "build": "yarn clean && yarn build:ts",
    "build:ts": "tsc -p ./",
    "clean": "rimraf ./dist && mkdir dist",
    "prepare": "husky install .github/husky",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@hidden-umbrella/eslint-config": "^1.0.0",
    "@types/app-root-path": "^1.2.4",
    "@types/convict": "^6.1.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=6"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@aero/centra": "^1.0.6",
    "@sapphire/decorators": "^2.2.0",
    "@sapphire/discord.js-utilities": "^2.0.2",
    "@sapphire/framework": "^2.0.0-pr-218.ee962943.0",
    "@sapphire/utilities": "^2.0.1",
    "app-root-path": "^3.0.0",
    "convict": "^6.2.0",
    "discord.js": "^12.5.3"
  }
}
