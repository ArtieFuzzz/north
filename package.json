{
  "name": "astraea",
  "version": "1.1.7",
  "main": "build/index",
  "author": "ArtieFuzzz#8298",
  "license": "MIT",
  "private": true,
  "description": "Discord bot using @sapphire/framework",
  "scripts": {
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --cache --report-unused-disable-directives --ext .ts src/ --fix",
    "build": "yarn clean && yarn build:ts",
    "build:ts": "tsc -p ./",
    "clean": "rimraf ./build && mkdir build",
    "commit": "git-cz",
    "commit:lint": "commitlint",
    "postinstall": "husky install .github/husky",
    "upgrade": "yarn upgrade-interactive",
    "contrib:add": "all-contributors add",
    "contrib:gen": "all-contributors generate",
    "contrib:check": "all-contributors check"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@hidden-umbrella/eslint-config": "^1.0.2",
    "@types/app-root-path": "^1.2.4",
    "@types/convict": "^6.1.1",
    "@types/node": "^16.4.13",
    "@types/node-fetch": "^2.5.12",
    "all-contributors-cli": "^6.20.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=6"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@aero/ksoft": "^4.0.0-rc1",
    "@sapphire/decorators": "^2.2.0",
    "@sapphire/discord.js-utilities": "^2.0.2",
    "@sapphire/fetch": "^1.0.5",
    "@sapphire/framework": "2.0.0-next.eebecfb6.0",
    "@sapphire/type": "^2.0.11",
    "@sapphire/utilities": "^2.0.1",
    "@tmware/status-rotate": "^1.2.8",
    "app-root-path": "^3.0.0",
    "bufferutil": "^4.0.3",
    "convict": "^6.2.0",
    "discord.js": "^12.5.3",
    "utf-8-validate": "^5.0.5"
  }
}
