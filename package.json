{
  "name": "north",
  "version": "1.1.3",
  "main": "dist/index",
  "author": "ArtieFuzzz#8298",
  "license": "MIT",
  "private": true,
  "description": "Discord bot using @sapphiredev/framework",
  "scripts": {
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "build": "yarn clean && yarn build:ts",
    "build:ts": "tsc -p ./",
    "clean": "rimraf ./dist && mkdir dist",
    "commit": "yarn dlx git-cz",
    "postinstall": "husky install .github/husky",
    "upgrade": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@hidden-umbrella/eslint-config": "^1.0.1",
    "@types/app-root-path": "^1.2.4",
    "@types/convict": "^6.1.1",
    "@types/node": "^16.4.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "husky": "^7.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=6"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@aero/centra": "^1.0.6",
    "@aero/ksoft": "^4.0.0-rc1",
    "@sapphire/decorators": "^2.2.0",
    "@sapphire/discord.js-utilities": "^2.0.2",
    "@sapphire/framework": "2.0.0-next.9790c48d.0",
    "@sapphire/pieces": "1",
    "@sapphire/plugin-api": "^2.2.1-next.03bbed6.0",
    "@sapphire/plugin-subcommands": "^1.0.6",
    "@sapphire/type": "^2.0.11",
    "@sapphire/utilities": "^2.0.1",
    "app-root-path": "^3.0.0",
    "convict": "^6.2.0",
    "discord-api-types": "^0.20.2",
    "discord.js": "^12.5.3",
    "yiff": "^3.1.2"
  }
}
